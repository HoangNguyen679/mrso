<div class="content">
  <div class="register">
    <div class="col-md-10 col-md-offset-1">
    	<div class="content">
        <div class="register-but">
		     	<%= form_for @review do |f| %>
				  <%= field_set_tag nil, class: "form-group" do %>
				 
				  <%= f.text_area :content, rows: 10, cols: 25, :class => "tinymce" %>
				  <% end %>
				  <div class="actions">
					  <%= f.submit "Done" %>
					</div>
				<% end %>
			</div>
		</div>
    </div>
    <div class="clearfix"> </div>
  </div>
</div>
<br><br>
<%= tinymce %>
